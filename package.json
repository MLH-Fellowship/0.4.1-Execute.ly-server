{
  "name": "ts-express-jest",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/moxios": "^0.4.9",
    "@types/node": "^12.7.12",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.8",
    "jest": "^22.4.4",
    "lint-staged": "^9.4.2",
    "moxios": "^0.4.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^22.4.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "main": "dist/index.js",
  "scripts": {
    "obsolete:prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "lint": "tsc --noEmit && eslint '{src,__test__}/**/*.{js,ts,tsx}' --quiet --fix",
    "prestart": "npm run build",
    "start": "node .",
    "dev:server": "nodemon",
    "dev:server-debug": "node -r ts-node/register --inspect=5858 src/index.ts",
    "test": "jest",
    "dev:tdd": "node --inspect=5859 node_modules/.bin/jest --runInBand --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "body-parser": "^1.19.0"
  }
}
