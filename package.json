{
  "name": "executely-server",
  "version": "0.0.0",
  "description": "server for the Executely project of pod",
  "keywords": [],
  "author": "REACTIVE SEA-SON",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.3",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.9",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "axios": "^0.19.2",
    "cloudinary": "^1.21.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.7",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp ocr-test.json dist/ocr-test.json",
    "lint": "tsc --noEmit && eslint {src,__tests__}/**/*.{js,ts,tsx} --quiet --fix",
    "prestart": "npm run build",
    "start": "node dist/src/index.js",
    "dev:server": "nodemon",
    "dev:server-debug": "node -r ts-node/register --inspect=5858 src/index.ts",
    "test": "jest",
    "dev:tdd": "node --inspect=5859 node_modules/.bin/jest --runInBand --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "@google-cloud/vision": "^1.11.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.3",
    "multer": "^1.4.2",
    "cors": "^2.8.5",
    "axios": "^0.19.2"
  }
}
