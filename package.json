{
  "name": "executely-server",
  "version": "0.0.0",
  "description": "server for the Executely project of pod",
  "keywords": [],
  "author": "REACTIVE SEA-SON",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.12",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.8",
    "jest": "^22.4.4",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^22.4.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit && eslint '{src,__test__}/**/*.{js,ts,tsx}' --quiet --fix",
    "prestart": "npm run build",
    "start": "node dist/src/index.js",
    "dev:server": "nodemon",
    "dev:server-debug": "node -r ts-node/register --inspect=5858 src/index.ts",
    "test": "jest",
    "dev:tdd": "node --inspect=5859 node_modules/.bin/jest --runInBand --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "body-parser": "^1.19.0"
  }
}
